import java.util.Scanner;

public class Zadacha_4 {

	public static void main(String[] arg) {

		Scanner test = new Scanner(System.in, "UTF-8");

		System.out.println("Enter two numbers to calculate N!/K! equation: ");
		int N = test.nextInt();
		int K = test.nextInt();
		long N_fact = 1;
		long K_fact = 1;
		while (true) {
			if (N == 1) {
				break;
			}
			N_fact *= N;
			N--;

		}
		System.out.println("n!=" + N_fact);
		test.close();
	}
}
