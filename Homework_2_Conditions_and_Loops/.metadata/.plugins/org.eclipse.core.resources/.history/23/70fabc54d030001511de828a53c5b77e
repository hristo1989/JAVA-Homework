		double B = b * b - 4 * a * c;
		double B1= Math.sqrt((b * b) - (4 * a * c));
		double x = 4;
		double x1 = (-b + B1 ) / (2 * c);
		double x2 = (-b - B1 )/ (2 * c);
		System.out.println("Math.sqrt(" + x + ")=" + Math.sqrt(x));
	System.out.println(B1);
	test.close();
	
	}
	
}
